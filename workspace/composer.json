{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.1.*",
		"firebase/php-jwt": "~2.0",
		"guzzlehttp/guzzle": "5.3.0",
		"guzzlehttp/oauth-subscriber": "0.2.0",
		"ktamas77/firebase-php": "dev-master",
		"nesbot/carbon": "^1.19",
		"fzaninotto/faker": "^1.4",
		"barryvdh/laravel-debugbar": "^2.0",
		"mockery/mockery": "^0.9.4",
		"twilio/sdk": "^4.2",
		"fedeisas/laravel-mail-css-inliner": "@dev",
    "ignited/laravel-omnipay": "2.*",
		"studysocial/omnipay-braintree": "dev-master",
		"studysocial/omnipay-paypal": "dev-master",
		"studysocial/omnipay-stripe": "dev-master#b2b7861096f54a81dcc64645d7ddcd9bdd70272f",
    "studysocial/php-saml": "dev-master",
    "studysocial/laravel-saml2": "dev-master",
		"league/csv": "^7.1",
		"doctrine/dbal": "^2.5",
		"opentok/opentok": "2.3.x",
		"maatwebsite/excel": "~2.0.0",
    "davibennun/laravel-push-notification": "dev-laravel5"
  },
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
    "repositories": [
    {
        "type": "git",
        "url": "https://github.com/studysocial/omnipay-braintree"
    },
    {
        "type": "git",
        "url": "https://github.com/studysocial/omnipay-stripe"
    },
    {
        "type": "git",
        "url": "https://github.com/studysocial/omnipay-paypal"
    },
    {
      "type": "git",
      "url": "https://github.com/studysocial/laravel-saml2"
    },
      {
        "type": "git",
        "url": "https://github.com/studysocial/php-saml"
      }
    ],
	"config": {
		"preferred-install": "dist"
	}
}
